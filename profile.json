{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Profile",
  "description": "The users profile",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "email": {
      "description": "The unique identifier for a product",
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
  },
  "required": ["email", "phone"],
  "readonly": ["name"]
}
